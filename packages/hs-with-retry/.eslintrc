{
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "script"
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "warn",
    "strict": [
      2,
      "global"
    ],
    "no-unused-vars": "warn"
  }
}
